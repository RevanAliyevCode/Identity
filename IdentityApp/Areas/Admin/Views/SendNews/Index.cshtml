@using IdentityApp.Areas.Admin.Models.Users
@model List<UsersVM>

<div class="p-4">
    <a type="button" class="btn btn-primary mb-4" asp-controller="SendNews" asp-action="SendMail">Send mail</a>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Email</td>
                </tr>
            }
        </tbody>
    </table>
</div>